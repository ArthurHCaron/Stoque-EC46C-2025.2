<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.prototipo.LoginController">
   <children>
      <Label layoutX="163.0" layoutY="35.0" text="Login">
         <font><Font size="20.0" /></font>
      </Label>

      <Label layoutX="69.0" layoutY="110.0" text="Usuário">
         <font><Font size="14.0" /></font>
      </Label>
      <TextField fx:id="cxUsuario" layoutX="130.0" layoutY="105.0" prefHeight="29.0" prefWidth="183.0" promptText="Digite seu usuário">
         <font>
            <Font size="14.0" />
         </font></TextField>

      <Label layoutX="78.0" layoutY="150.0" text="Senha">
         <font><Font size="14.0" /></font>
      </Label>
      <PasswordField fx:id="cxSenha" layoutX="130.0" layoutY="145.0" prefHeight="29.0" prefWidth="183.0" promptText="Digite sua senha">
         <font>
            <Font size="14.0" />
         </font></PasswordField>

      <Button fx:id="btEntrar" layoutX="257.0" layoutY="195.0" onAction="#handleLogin" text="Entrar">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btCadastrar" layoutX="130.0" layoutY="195.0" onAction="#handleCadastro" text="Cadastrar-se">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
