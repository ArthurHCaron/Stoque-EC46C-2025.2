         <font>
            <Font size="14.0" />
         </font></Label>
      <TextField fx:id="cxLocalizacao" layoutX="143.0" layoutY="210.0">
         <font>
            <Font size="14.0" />
         </font></TextField>

      <Button fx:id="btSalvar" layoutX="260.0" layoutY="269.0" onAction="#handleSalvar" text="Salvar">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btCancelar" layoutX="143.0" layoutY="269.0" onAction="#handleCancelar" text="Cancelar">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
    
</AnchorPane>
